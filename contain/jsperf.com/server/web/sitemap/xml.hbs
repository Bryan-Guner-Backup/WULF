<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>{{scheme}}://{{domain}}/</loc>
		<changefreq>daily</changefreq>
	</url>
	<url>
		<loc>{{scheme}}://{{domain}}/browse</loc>
		<changefreq>daily</changefreq>
	</url>
	<url>
		<loc>{{scheme}}://{{domain}}/faq</loc>
	</url>

	{{#each items}}
		<url>
			<loc>{{../scheme}}://{{../domain}}/{{slug}}{{#compare revision 1}}/{{revision}}{{/compare}}</loc>
		</url>
	{{/each}}
</urlset>
